if photo.url
  div.photo-time #{photo.time.date}
  img(src=photo.url,id=photo.id)
  if (is_my_user)
    .actions
        if(liked_by)
          a(class="action-remove-like custom-link") вам нравится
        else
          a(class="action-like custom-link") нравится
        span.like-counter-box
          i.fui-heart &nbsp;
          if photo.likes >0
            span.like-counter.likes.my-likes(data-likers-url=photo.likers_url) #{photo.likes}
          else 
            span.like-counter.likes.my-likes(data-likers-url=photo.likers_url)
        //- a(class="custom-blue-link new-ava") на аватарку
        a(class="custom-link remove-photo") удалить
  else
    if(liked_by)
      a(class="action-remove-like custom-link") вам нравится
    else
      a(class="action-like custom-link") нравится
    span.like-counter  #{photo.likes}
else
  div.photo-time загрузка...
  img(src="/static/img/searchpreloader.GIF", class='loading')