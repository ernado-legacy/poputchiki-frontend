each status in statuses.models
  .statusBlock(data-status='#{status.get('id')}')
    a(href="/user/#{users[status.get("user")].get("id")}")
      img(src='#{users[status.get("user")].get("avatar_url")}')
    .profileInfoBox.leftPart     
      .name-in-profile #{users[status.get("user")].get('name')}, 23
      .city-in-profile=users[status.get("user")].get('city')
      if(users[status.get("user")].get('is_sponsor'))
        .money-icon.mg-icon
      if(users[status.get("user")].get('is_sponsor'))
        .house-icon.hg-icon
    .rightPart
      .status=status.get('text')
      a(class='writeBtn fui-bubble', href="/message/#{users[status.get("user")].get("id")}")
      .controlLinks.clearfix
        .like(data-like="true")
        .note пожаловаться
        .share поделиться
        .likes
